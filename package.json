{
  "name": "primatyre",
  "license": "MIT",
  "scripts": {
    "queries": "ts-node ./src/queries.ts",
    "caching": "ts-node ./src/caching.ts",
    "migrate": "prisma migrate dev --name lazy",
    "push": "prisma db push",
    "devIndex": "tsx index.ts",
    "devSeed": "tsx seed.ts",
    "version:update": "tsx scripts/update-version.ts",
    "predev": "npm run version:update",
    "prebuild": "npm run version:update",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@prisma/client": "6.7.0",
    "@prisma/extension-accelerate": "^1.3.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "date-fns-tz": "^3.2.0",
    "dotenv": "16.5.0",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.10"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.15.18",
    "prisma": "^6.7.0",
    "ts-node": "10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.2"
  },
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true
  },
  "include": [
    "src"
  ],
  "version": "1.5.2",
  "description": "This Project is PrimaTyre from Prima Jaya Persada",
  "main": "src/index.js",
  "keywords": [],
  "author": ""
}